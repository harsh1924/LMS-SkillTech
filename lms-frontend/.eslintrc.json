{
  "extends": [
    // "next/babel",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:import/errors"
  ],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "off",
    "no-unused-vars": "off",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          ["sibling", "parent"],
          "index"
        ],
        "newlines-between": "always"
      }
    ]
  }
}